{
    "id": "status_core",
    "version": "1.0.0",
    "engineVersion": "0.10.0",
	"authors": [
		"AcousticJamm",
		"HyperBoid"
	],
	"optionalDependencies": [
		"magical-glass"
	],
	"config": {
		// If true, it will color the status effect icon's colors to the party member that has them.
		"match_color": true,
		
		// If true, the status effect menu will show up for the player when they hold C.
		"status_menu": true,
		
		// If true, no party member may exceed this many effects.
		"max_effects": 2,
		
		// Library dependencies stuff
		"magical-glass": true
	},
}